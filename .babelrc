{
    "presets": [
        [
            "next/babel",
            {
                "styled-jsx": {
                    "plugins": [
                        ["styled-jsx-plugin-sass"]
                    ]
                }
            }
        ]
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
            }
        ],
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}